module Stave
  class Pitch < Core::Lookup
    attr_reader ordering: Integer
    attr_reader semitones: Integer

    def +: (Integer) -> Pitch

    def -: (Integer) -> Pitch

    def self.order: -> Array[Symbol]

    def self.a: -> Pitch
    def self.b: -> Pitch
    def self.c: -> Pitch
    def self.d: -> Pitch
    def self.e: -> Pitch
    def self.f: -> Pitch
    def self.g: -> Pitch
  end
end
